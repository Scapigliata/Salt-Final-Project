{
    "env": {
        "browser": true,
        "es6": true,
        "jest": true
    },
    "parser": "babel-eslint",
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/jsx-filename-extension": 0,
        "no-use-before-define": 0,
        "indent": [2, 2, {
            "SwitchCase": 1
        }],
        "no-undef": 0,
        "arrow-parens": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-nested-ternary": 0,
        "no-shadow": 0,
        "max-len": 0,
        "implicit-arrow-linebreak": 0,
        "no-confusing-arrow": 0,
        "jsx-quotes": 0,
        "global-require": 0,
        "react/prop-types": 0,
        "no-unused-vars": 0,
        "react/jsx-boolean-value": 0,
        "import/extensions": 0,
        "react/no-array-index-key": 0,
        "react/jsx-curly-brace-presence": 0,
        "quote-props": 0,
        "quotes": 0,
        "no-restricted-globals": 0,
        "object-curly-newline": 0,
        "arrow-body-style": 0,
        "no-global-assign": 0,
        "prefer-const": 0,
        "no-plusplus": 0,
        "no-unused-expressions": 0
    }
}